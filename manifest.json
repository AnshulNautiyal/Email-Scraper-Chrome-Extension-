{
    "manifest_version": 2,
    "name": "Email Scraper",
    "description": "Scrape all the email from a website",
    "version": "1.0",
    "browser_action": {
      "default_popup": "index.html"
    },
  "permissions": [
    "activeTab",
    "<all_urls>"
],
"background": {
  "scripts": ["index.js"]
}

  }